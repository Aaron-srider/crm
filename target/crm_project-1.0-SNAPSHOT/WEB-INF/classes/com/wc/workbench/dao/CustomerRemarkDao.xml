<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wc.workbench.dao.CustomerRemarkDao">
    <insert id="save">
        insert into tbl_customer_remark
        (
            `noteContent`,
            `customerId`,
            `id`,
            `createTime`,
            `createBy`,
            `editFlag`
        )
            value
            (
            #{noteContent},
            #{customerId},
            #{id},
            #{createTime},
            #{createBy},
            #{editFlag}
            )
    </insert>
  	
</mapper>

































