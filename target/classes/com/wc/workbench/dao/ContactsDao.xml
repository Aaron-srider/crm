<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wc.workbench.dao.ContactsDao">
    <insert id="save">
        insert into `tbl_contacts` (
            `source`,
            `fullname`,
            `appellation`,
            `email`,
            `mphone`,
            `job`,
            `contactSummary`,
            `nextContactTime`,
            `owner`,
            `description`,
            `address`,
            `id`,
            `customerId`,
            `createBy`,
            `createTime`,
            `birth`
        )
            value
            (
                #{source},
                #{fullname},
                #{appellation},
                #{email},
                #{mphone},
                #{job},
                #{contactSummary},
                #{nextContactTime},
                #{owner},
                #{description},
                #{address},
                #{id},
                #{customerId},
                #{createBy},
                #{createTime},
                #{birth}
            )
    </insert>
  	
</mapper>

































