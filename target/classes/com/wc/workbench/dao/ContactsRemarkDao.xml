<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wc.workbench.dao.ContactsRemarkDao">

    <insert id="save">
        insert into `tbl_contacts_remark`
        (
            `noteContent`,
            `contactsId`,
            `id`,
            `createTime`,
            `createBy`,
            `editFlag`
        )
        value
        (
           #{noteContent},
           #{contactsId},
           #{id},
           #{createTime},
           #{createBy},
           #{editFlag}
        )
    </insert>
</mapper>

































